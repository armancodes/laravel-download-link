{
    "name": "armancodes/laravel-download-link",
    "description": "A simple Laravel package for generating download links with options such as expire time, IP restrictions, etc.",
    "keywords": [
        "armancodes",
        "laravel-download-link"
    ],
    "homepage": "https://github.com/armancodes/laravel-download-link",
    "license": "MIT",
    "authors": [
        {
            "name": "Arman Ahmadi",
            "email": "me@armancodes.com",
            "homepage": "https://github.com/armancodes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Armancodes\\DownloadLink\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Armancodes\\DownloadLink\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armancodes\\DownloadLink\\DownloadLinkServiceProvider"
            ],
            "aliases": {
                "DownloadLink": "Armancodes\\DownloadLink\\Facades\\DownloadLinkGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
